<div class="fadeIn">
	<div class="container-fluid">
		<form class="form-dark" name="frmRoute">
			<div class="form-group">
				<label>
					PHONE
				</label>
				<div class="input-group">
					<div class="input-group-addon"><i class="fa fa-phone"></i></div>
					<input type="text" class="form-control" />
				</div>
			</div>
			<h4 class="form-subheader">
				{{vmRDC.rdc.locationName}} LOCATION
			</h4>
			
			<div class="form-group">
				<label>
					Address
				</label>
				<div class="input-group">
					<div class="input-group-addon"><i class="fa fa-map-marker"></i></div>
					<input type="text" class="form-control" />
				</div>
			</div>

			<div class="form-group">
				<label>
					City
				</label>
				<div class="input-group">
					<div class="input-group-addon"><i class="fa fa-map-marker"></i></div>
					<input type="text" class="form-control" />
				</div>
			</div>

			<div class="form-group">
				<label>
					STATE
				</label>
				<div class="input-group">
					<div class="input-group-addon"><i class="fa fa-map-marker"></i></div>
					<input type="text" class="form-control" />
				</div>
			</div>

			<div class="form-group">
				<label>
					ZIP
				</label>
				<div class="input-group">
					<div class="input-group-addon"><i class="fa fa-map-marker"></i></div>
					<input type="text" class="form-control" />
				</div>
			</div>

			<div class="form-group">
				<button class="btn btn-primary">Update RDC Details</button>
			</div>

			<h4 class="form-subheader">
